{
    "name": "pusparaaz/iap-validator",
    "description": "Composer package to validate in-app purchase receipts made to iTunes or Google through mobile apps.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Pusparaj Bhattarai",
            "email": "mail@pusparaaz.com.np",
            "homepage": "https://pusparaaz.com.np",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "iap-validator@pusparaaz.com.np"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "phpunit/phpunit-mock-objects": "~3.0",
        "illuminate/support": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "PuspaRaaz\\IAPValidator\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
